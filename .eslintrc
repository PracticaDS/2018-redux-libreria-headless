{
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "import",
    "jest"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "semi": 0,
    "padded-blocks": 0,
    "comma-dangle": 0,
    "arrow-parens": 0
  },
  "env": {
    "node": true,
    "jest/globals": true
  }
}